apiVersion: v1
kind: Service
metadata:
  name: apache
  labels:
    name: apache
spec:
  type: NodePort
  ports:
    - port: 80
      targetPort: 80
      nodePort: {{ apache_external_nodeport }}
      protocol: TCP
  selector:
    name: apache
